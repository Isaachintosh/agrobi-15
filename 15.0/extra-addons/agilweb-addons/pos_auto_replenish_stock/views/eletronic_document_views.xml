<?xml version="1.0" encoding="utf-8"?>
  <odoo>
      
    <!-- View eletronic.document form -->
    <record id="view_eletronic_document_form" model="ir.ui.view">
        <field name="name">view.eletronic.document.form</field>
        <field name="model">eletronic.document</field>
        <field name="inherit_id" ref="l10n_br_eletronic_document.view_eletronic_document_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_generate_xml']" position="attributes">
              <attribute name="groups">
                base.group_user
              </attribute>
            </xpath>
        </field>
    </record>

  </odoo>