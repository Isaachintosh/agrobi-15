<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- View product.template View Tree -->
    <record id="view_product_template_tree" model="ir.ui.view">
        <field name="name">view.product.template.tree</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button string="Importar Quantidades" name="open_product_qty_wizard" type="object" class="oe_highlight"/>
            </xpath>
        </field>
    </record>

</odoo>
